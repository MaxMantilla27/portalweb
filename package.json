    "@angular-devkit/build-angular": "^13.3.5",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/video.js": "^7.3.40",
    "jasmine-core": "~3.10.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",

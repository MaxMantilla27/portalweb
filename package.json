{
  "name": "portal-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run partytown && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run PortalWeb:serve-ssr",
    "serve:ssr": "node dist/PortalWeb/server/main.js",
    "build:ssr": "npm run partytown && ng build && ng run PortalWeb:server && gzipper c ./dist --include js,css,html --output-file-format [filename].[ext] ./dist-compressed/ ",
    "prerender": "ng run PortalWeb:prerender",
    "partytown": "partytown copylib public/~partytown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/localize": "~13.1.0",
    "@angular/material": "^13.1.3",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@builder.io/partytown": "^0.7.5",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@cloudflare/stream-angular": "^1.0.1",
    "@microsoft/signalr": "^6.0.7",
    "@ng-bootstrap/ng-bootstrap": "^12.0.2",
    "@nguniversal/express-engine": "^13.0.2",
